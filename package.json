{
  "name": "@hsborges/dependencies-analyzer",
  "version": "1.4.0",
  "author": "Hudson Silva Borges <hudsonsilbor@gmail.com>",
  "description": "Dependencies analyzer is a tool for extracting the history evolution of the dependencies described in the package.json and bower.json.",
  "homepage": "https://github.com/hsborges/dependencies-analyzer#readme",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hsborges/dependencies-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/hsborges/dependencies-analyzer/issues"
  },
  "directories": {
    "bin": "./bin"
  },
  "bin": {
    "dependencies-analyzer": "bin/analyzer"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "commander": "^5.0.0",
    "debug": "^4.1.1",
    "globby": "^11.0.0",
    "json2csv": "^5.0.0",
    "lodash": "^4.17.15",
    "read-package-json": "^2.1.1",
    "simple-git": "^1.132.0",
    "tmp-promise": "^2.0.2"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
